@page "/profil"
@if (CurrentUser != null)
{
    <h3>UserProfil</h3>
    <p>Email : @CurrentUser.Email</p>
    <p>Nom : @CurrentUser.FirstName</p>
    <p>Prenom : @CurrentUser.LastName</p>
    <button @onclick=ModifierProfil>
        Modifier le profil
    </button>
}else{
    Nav.NavigateTo("/login");
    ToastService.ShowInfo("Vous devez être connecté pour accéder à cet url.");
}
